## Project settings
PROJECT = TestProgram

## Source Directories
AVRTEST_DIR = AVRTest

## User Sources
SOURCES = Temp.cpp

## Compiler Flags
CPPFLAGS = -I $(AVRTEST_DIR)/include
CXXFLAGS = -Wall -Wextra
FLAGS = $(CPPFLAGS) $(CXXFLAGS)
CMND = $(CXX) $(FLAGS)

all: AVRTestAll_main.o $(SOURCES)
	$(CMND) $^ -o $(PROJECT)

clean:
	rm -rf $(PROJECT) *.o

##############################################################

AVRTestAll.o: $(AVRTEST_DIR)/src/AVRTestAll.cpp
	$(CMND) -c $^ -o $@

AVRTestAll_main.o: $(AVRTEST_DIR)/src/AVRTestAll_main.cpp
	$(CMND) -c $^ -o $@